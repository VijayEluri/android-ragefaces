	<!-- Help dialog -->
	<string name="help">Help</string>
	<!-- Help dialog contents -->
	<string name="help_msg">Basic stuff: Click a face and then do what you want with it.\n\nTo insert a face into a current conversation:\n1. Open the menu and click \"Attach\"\n2. Click \"Pictures\"\n3. Click \"Rage Faces\"</string>
	
	<!-- About dialog -->
	<!-- Label for menu option "About" -->
	<string name="about">About</string>
	<!-- "About" dialog contents.  Make sure to keep "\n\n" as a newline between each section.  Be sure to leave the %1$s, %2$s, etc. in!  They are important!
		%1$s == App version
		%2$s == list of contributors
		%3$s == list of translators -->
	<string name="about_msg">Rage Faces %1$s.\n\nWritten by rkcr.\n\nContributors: %2$s\n\nTranslators: %3$s\n\nApp icon by TheFunkyMonk.\n\nTest helper: mystikalghost</string>

