	<!-- Help dialog -->
	<string name="help">Hjälp</string>
	<!-- Help dialog contents -->
	<string name="help_msg">Grundläggande: Klicka på en bild och gör sedan vad du vill med den.\n\nFör att lägga till en bild i en pågående konversation:\n1. Öppna menyn och klicka \"Lägg till\"\n2. Klicka \"Bilder\"\n3. Klicka \"Rage Faces\"</string>
	
	<!-- About dialog -->
	<!-- Label for menu option "About" -->
	<string name="about">Om</string>
	<!-- "About" dialog contents.  Make sure to keep "\n\n" as a newline between each section.  Be sure to leave the %1$s, %2$s, etc. in!  They are important!
		%1$s == App version
		%2$s == list of contributors
		%3$s == list of translators -->
	<string name="about_msg">Rage Faces %1$s.\n\nSkapad av rkcr.\n\nMedarbetare: %2$s\n\nÖversättare: %3$s\n\nAppikon av TheFunkyMonk.\n\nTestare: mystikalghost</string>
	
	<!-- Error strings -->
